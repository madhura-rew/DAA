// Write your code here...
void read1(int a[],int n) {
	printf("Enter %d elements : ",n);
	for (int i = 0; i < n;i++){
		scanf("%d",&a[i]);
	}
}

int linearSearch(int a[], int start, int end, int key){
	if (start > end) {
		return -1;
	}
	if (a[start] == key){
		return start;
	}
	return linearSearch(a, start + 1, end, key);
}
